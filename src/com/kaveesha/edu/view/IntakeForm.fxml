            <TableColumn fx:id="colIntakeName" prefWidth="174.0" text="Intake Name" />
            <TableColumn fx:id="colStartDate" prefWidth="251.0" text="Start Date" />
            <TableColumn fx:id="colOption" prefWidth="281.0" text="Operation" />
